{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --host 0.0.0.0",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && npx prisma generate",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate deploy",
    "prisma:studio": "npx prisma studio",
    "prisma:schema": "npx prisma db push",
    "prisma": "npm run prisma:generate && npm run prisma:migrate && npm run prisma:schema",
    "vercel-build": "npm run prisma:generate && npm run build"
  },
  "dependencies": {
    "@nuxt/fonts": "^0.11.2",
    "@nuxt/icon": "^1.12.0",
    "@pinia/nuxt": "^0.11.0",
    "@prisma/nuxt": "^0.3.0",
    "bcryptjs": "^2.4.3",
    "gsap": "^3.13.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "notivue": "^2.4.5",
    "nuxt": "^3.17.2",
    "pinia": "^3.0.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@nuxt/ui": "^3.3.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
